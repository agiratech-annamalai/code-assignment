{
  "name": "code-assignment",
  "version": "0.0.1",
  "description": "code assignment",
  "keywords": [
    "typescript",
    "starter"
  ],
  "author": "Annamalai <annamalai@agiratech.com>",
  "license": "ISC",
  "engineStrict": true,
  "engines": {
    "node": "^12",
    "yarn": "^1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/agiratech-annamalai/code-assignment.git"
  },
  "os": [
    "darwin",
    "linux",
    "win32"
  ],
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*.d.ts",
    "dist/**/*.js"
  ],
  "scripts": {
    "test-var": "echo $npm_execpath",
    "preinstall": "node -e \"if(!/yarn\\.js$/.test(process.env.npm_execpath))throw new Error('Use yarn')\"",
    "postinstall": "test -n \"$DOCKER_ENV\" ||  yarn run build",
    "clean:dist": "rm -rf dist && rm -f .buildcache",
    "prebuild": "yarn run lint && yarn run clean:dist",
    "build": "yarn run tsc -p tsconfig.json",
    "postlint": "yarn run format:check",
    "lint": "yarn run eslint . --ext .ts",
    "lint:fix": "yarn run eslint . --fix --ext .ts",
    "format:fix": "yarn run prettier --write \"./src/*.{ts,js,json,md}\"",
    "format:check": "yarn run prettier --list-different \"./src/*.{ts,js,json,md}\"",
    "start": "yarn node dist/index.js",
    "start:dev": "nodemon --exec ./node_modules/.bin/ts-node -- ./src/index.ts"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/node": "^14.0.13",
    "@typescript-eslint/eslint-plugin": "^3.2.0",
    "@typescript-eslint/parser": "^3.2.0",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.13.2",
    "eslint-plugin-prefer-arrow": "^1.2.1",
    "eslint-plugin-prettier": "^3.1.3",
    "prettier": "^2.0.5",
    "ts-node": "^8.10.2",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "@types/express": "^4.17.7",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "squiss-ts": "^4.1.0",
    "yarn": "^1.22.4"
  }
}
